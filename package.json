{
  "name": "@realmikekelly/exa-cli",
  "version": "1.0.3",
  "description": "CLI for Exa AI search via GAP (Gated Agent Proxy). Provides web search, code search, deep search, URL crawling, LinkedIn search, company research, and deep research capabilities.",
  "type": "module",
  "module": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikekelly/exa-cli.git"
  },
  "bin": {
    "exa": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "exa",
    "search",
    "cli",
    "gap",
    "websearch",
    "ai",
    "research",
    "linkedin"
  ],
  "author": "Mike Kelly",
  "scripts": {
    "build": "tsc",
    "build:cli": "tsc && chmod +x dist/cli.js",
    "watch": "tsc --watch",
    "dev": "tsx src/cli.ts",
    "prepublishOnly": "npm run build && chmod +x dist/cli.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "agnost": "^0.1.7",
    "axios": "^1.7.8",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "https-proxy-agent": "^7.0.6",
    "mcp-handler": "^1.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@smithery/cli": "^1.4.4",
    "@types/node": "^20.11.24",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vercel": "^37.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
